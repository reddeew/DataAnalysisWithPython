    "import datetime\n",
    "\n",
    "# Current Datetime\n",
    "now = datetime.datetime.today()\n",
    "print(f\"Current Datetime : {now}\")\n",
    "\n",
    "# Date\n",
    "dateStr = now.strftime(\"%Y-%m-%d\")\n",
    "print(f\"Current Date : {dateStr}\")\n"
